{
  "name": "cotizaciones",
  "version": "1.0.0",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build:web": "tsc -b && vite build",
    "build": "npm run build:web && electron-builder",
    "start": "electron ."
  },
"build": {
  "appId": "com.kenner.cotizaciones",
  "productName": "Cotizaciones",
  "win": {
    "forceCodeSigning": false,
    "signAndEditExecutable": false,
    "target": ["nsis"]
  },
  "files": [
    "main.cjs",
    "preload.js",
    "dist/**/*",
    "package.json"
  ],
  "directories": {
    "buildResources": "assets"
  }
}
,
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.6.0",
    "vite": "^7.3.0",
    "typescript": "^5.3.0"
  }
}
